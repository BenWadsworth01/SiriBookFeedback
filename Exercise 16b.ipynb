   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib tk\n",
    "\n",
    "z = np.linspace(-1,1)\n",
    "\n",
    "def div_z(z):\n",
    "    return 24*z**2 + 8*z\n",
    "\n",
    "plt.xlabel('z')\n",
    "plt.ylabel('Div along z slice')\n",
    "plt.title('Divergence on each z = constant slice')\n",
    "plt.grid('on')\n",
    "\n",
    "plt.plot(z, div_z(z))\n",
    "plt.show()"
   ]
